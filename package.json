{
  "name": "@ansik/smartcar-integration",
  "version": "0.1.0",
  "description": "",
  "repository": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "node dist/index.js",
    "clean": "rimraf dist coverage",
    "build": "tsc --build tsconfig.build.json",
    "test:before": "sh test/setupOnce.sh",
    "test:after": "sh test/tearDownOnce.sh",
    "test:runner": "CONF_PATH=conf-e2e.json ./node_modules/.bin/jest --runInBand",
    "test:runner:unit": "CONF_PATH=conf-e2e.json ./node_modules/.bin/jest --config=./jest.config.unit.js",
    "test:runner:e2e": "CONF_PATH=conf-e2e.json ./node_modules/.bin/jest --config=./jest.config.e2e.js --detectOpenHandles --runInBand",
    "test": "npm run test:before && npm run test:runner && npm run test:after",
    "test:unit": "npm run test:before && npm run test:runner:unit && npm run test:after",
    "test:e2e": "npm run test:before && npm run test:runner:e2e && npm run test:after",
    "test:cov": "npm run test:before && npm run test:runner:unit -- --coverage && npm run test:after",
    "see-cov": "open coverage/lcov-report/index.html",
    "publish:before": "npm run clean && npm run test:unit && npm run build",
    "do-publish": "npm run publish:before && bash publish.sh"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ansik/sdk": "^0.1.1",
    "axios": "^0.19.2",
    "bluebird": "^3.7.2",
    "debug": "^4.1.1",
    "io-ts": "^1.10.4",
    "io-ts-types": "^0.5.5",
    "knex": "^0.20.8",
    "lodash": "^4.17.15",
    "smartcar": "^7.4.1",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/bluebird": "latest",
    "@types/bluebird-global": "latest",
    "@types/debug": "latest",
    "@types/jest": "latest",
    "@types/knex": "latest",
    "@types/lodash": "latest",
    "@types/node": "latest",
    "@types/uuid": "latest",
    "jest": "latest",
    "jest-circus": "latest",
    "jest-environment-node": "latest",
    "nock": "latest",
    "rimraf": "latest",
    "ts-jest": "latest",
    "typescript": "latest",
    "utility-types": "latest"
  }
}
